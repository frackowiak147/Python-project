def wybory(chose):
    if chose == "1":
        print("1 = tabela")
    elif chose == "2":
        print("2 - info")
    elif chose == "3":
        print("3 - korelacja")
    elif chose == "0":
        print("0 - zakoncz")
    elif (chose != "1" or chose != "2" or chose != "3" or chose != "0"):
        print("Zly wybor")

def wyswietl():
    print("Wybierz opcje ")
    print("1 - tabela ")
    print("2 - info ")
    print("3 - korelacja")
    print("0 - zakoncz ")
    


